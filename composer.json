{
    "name": "ethical-jobs/laravel-token-generator",
    "description": "Generate simple tokens",
    "homepage": "https://github.com/ethical-jobs/laravel-token-generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "EthicalJobs.com.au",
            "email": "info@ethicaljobs.com.au",
            "homepage": "http://ethicaljobs.com.au"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "autoload": {
        "psr-4": {
            "EthicalJobs\\Token\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "EthicalJobs\\Token\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "orchestra/testbench": "^3.8"
    }
}
